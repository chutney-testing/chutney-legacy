{
  "version": 1,
  "projects": {
    "chutney": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/chutney",
      "sourceRoot": "apps/chutney/src",
      "prefix": "chutney",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./monaco-webpack.config.js",
              "mergeStrategies": {
                "module.rules": "prepend"
              }
            },
            "outputPath": "dist/apps/chutney",
            "index": "apps/chutney/src/index.html",
            "main": "apps/chutney/src/main.ts",
            "polyfills": "apps/chutney/src/polyfills.ts",
            "tsConfig": "apps/chutney/tsconfig.app.json",
            "aot": true,
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "libs/assets/src/lib",
                "output": "./"
              },
              {
                "glob": "**/*",
                "input": "libs/assets/src/lib",
                "output": "assets"
              }
            ],
            "styles": ["libs/styles/src/lib/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/styles/src/lib/styles"]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/chutney/src/environments/environment.ts",
                  "with": "apps/chutney/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "customWebpackConfig": {
              "path": "./monaco-webpack.config.js",
              "mergeStrategies": {
                "module.rules": "prepend"
              }
            },
            "browserTarget": "chutney:build",
            "proxyConfig": "apps/chutney/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "chutney:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "chutney:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/chutney/src/**/*.ts",
              "apps/chutney/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/chutney/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/chutney"]
        }
      }
    },
    "chutney-e2e": {
      "root": "apps/chutney-e2e",
      "sourceRoot": "apps/chutney-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
